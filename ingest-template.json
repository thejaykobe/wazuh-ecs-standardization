      {
        "geoip": {
          "field": "data.srcip",
          "target_field": "GeoLocation",
          "properties": ["city_name", "country_name", "region_name", "location"],
          "ignore_missing": true,
          "ignore_failure": true
        }
      },
      {
        "geoip": {
          "field": "data.win.eventdata.ipAddress",
          "target_field": "GeoLocation",
          "properties": ["city_name", "country_name", "region_name", "location"],
